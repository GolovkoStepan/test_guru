<%= simple_form_for @user do |f| %>
  <%= f.input :first_name  %>
  <%= f.input :last_name  %>
  <%= f.input :middle_name  %>
  <%= f.input :email, placeholder: 'user@domain.com', input_html: { autocomplete: 'off' } %>
  <%= f.input :password, input_html: { autocomplete: 'off' } %>
  <%= f.input :password_confirmation, input_html: { autocomplete: 'off' } %>

  <%= f.submit 'Sign up' %>
<% end %>
